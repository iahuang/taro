{
    "compilerOptions": {
        "module": "es6",
        "target": "ES2016",
        "lib": [
            "es2016",
            "dom"
        ],
        "strict": true,
        "sourceMap": true,
        "jsx": "preserve",
        "outDir": "./dist",
        "moduleResolution": "node",
        "typeRoots": [
            "./node_modules/@types",
            "./typings"
        ],
        "downlevelIteration": true
    },
    "exclude": [
        "node_modules"
    ]
}